{
  "ConnectionStrings": {
    "DefaultConnection": "sslmode=Require;Trust Server Certificate=true;Host={HOST};Database={DATABASE};Port=5432;Pooling=true;Username={USERNAME};Password={PASSWORD};"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
